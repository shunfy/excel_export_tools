{
    "IncludeFilesAndPath": [
        "./test/excel"
    ],
    "ExcludeFileNames": [
        "Exclude.xlsx"
    ],
    "ExcludeSheetNames": [
        "<none>"
    ],
    "Export": [
        {
            "<type> can be [csv|json|js|ts|lua].": null,
            "type": "json",
            "OutputDir": "./test/global.json",
            "UseDefaultValueIfEmpty": false
        },
        {
            "type": "json",
            "OutputDir": "./test/json/",
            "UseDefaultValueIfEmpty": false
        },
        {
            "type": "json",
            "OutputDir": "./test/json-bytes.bytes",
            "ExtName": ".bytes",
            "UseDefaultValueIfEmpty": false
        },
        {
            "type": "js",
            "OutputDir": "./test/js/",
            "ExportTemple": "export const {name}={data}",
            "UseDefaultValueIfEmpty": false
        },
        {
            "type": "js",
            "OutputDir": "./test/global.js",
            "ExportTemple": "export const GConfig = {data}",
            "UseDefaultValueIfEmpty": false
        },
        {
            "type": "ts",
            "OutputDir": "./test/global.d.ts",
            "ExportTemple": "export namespace GConfig {\n\n{data}\n}",
            "UseDefaultValueIfEmpty": false
        },
        {
            "type": "ts",
            "OutputDir": "./test/ts/",
            "ExportTemple": "export {data}",
            "UseDefaultValueIfEmpty": true
        },
        {
            "type": "csv",
            "OutputDir": "./test/csv/",
            "UseDefaultValueIfEmpty": false
        }
    ],
    "CSVNameCellID": "B1",
    "see: http://momentjs.com/docs/#/parsing/string-format/": null,
    "DateFmt": "YYYY/MM/DD HH:mm:ss",
    "TinyDateFmt": "YYYY/MM/DD",
    "LineBreak": "\r\n",
    "FractionDigitsFMT": 6,
    "EnableTypeCheck": true,
    "EnableDebugOutput": true,
    "EnableExportCommentColumns": true,
    "EnableExportCommentRows": true
}