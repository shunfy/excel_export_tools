{
    "IncludeFilesAndPath": [
        "./test/excel"
    ],
    "ExcludeFileNames": [
        "C充值.xlsx",
        "M姓名库.xlsx"
    ],
    "ExcludeCsvTableNames": [
        "TestCase"
    ],
    "Export": {
        "OutputDir": "./test/csv/",
        "EnableHeader": true,
        "-comment": [
            "static-ts-node   : static code using typescript using by nodejs",
            "static-ts-client : static code using typescript using by web browser",
            "cpp-headers      : static header code."
        ],
        "HeaderType": "static-ts-node"
    },
    "CSVNameCellID": "B1",
    "see: https://www.npmjs.com/package/dateformat": false,
    "DateFmt": "yyyy/mm/dd HH:MM:ss",
    "TinyDateFmt": "yy/mm/dd",
    "LineBreak": "\n",
    "EnableTypeCheck": true,
    "EnableDebugOutput": true,
    "EnableExportCommentColumns": true,
    "EnableExportCommentRows": true
}